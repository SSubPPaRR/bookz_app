AlertDialog(
                    title: Text('Are you sure you want to delete?'),
                    content: Text(book.title),
                    actions: <Widget>[
                      OutlineButton(
                          onPressed: () {
                            Navigator.of(context).pop();
                          },
                          child: Text('No')),
                      FlatButton(onPressed: removeFromCart, child: Text('Yes')),
                    ],
                  );